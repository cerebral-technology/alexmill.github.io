---
layout: nil
category: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	
	<title type="text" xml:lang="en">{{ site.root_desc }}</title>
	<link type="application/atom+xml" href="http://alex.miller.im/feed/" rel="self"/>
 	<link type="text" href="http://alex.miller.im" rel="alternate"/>
 	<link rel="hub" href="http://pubsubhubbub.superfeedr.com/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://alex.miller.im</id>
	<author>
		<name>Alex Miller</name>
	</author>
	<rights>Copyright (c) 2014-2016, Alex Miller; all rights reserved.</rights>
	
	{% for post in site.posts limit:20 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://alex.miller.im{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://alex.miller.im{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
